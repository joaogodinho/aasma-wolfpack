to sheep-move
  if random-float 1 < SHEEP-MOVE-PROB [
    let target-pos one-of sheep-adjacent-positions
    if not any? wolves-on target-pos [
      move-to target-pos
    ]
  ]
end

to wolf-move-adjacent [ target-patch ]
  move-to target-patch
end

to wolf-move-random
  wolf-move-adjacent one-of wolf-adjacent-positions
end
 
    
  
  
  